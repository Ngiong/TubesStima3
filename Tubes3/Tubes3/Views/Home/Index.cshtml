@{
    ViewBag.Title = "Home Page";
}

<h1>Tweets : </h1>

<div>
    <ol class="round">
        @foreach (dynamic item in ViewBag.Tweets)
        {
            <li>
                @item.getText()
            </li>
            <li>
                @item.getCategory()
            </li>
        }
    </ol>
</div>
